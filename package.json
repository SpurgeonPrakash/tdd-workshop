{
  "name": "tdd-workshop",
  "version": "1.0.0",
  "description": "Materials for the workshop \"Building Quality JavaScript With Test-Driven Development\", by Steven J Hicks.",
  "private": true,
  "scripts": {
    "verify": "node ./module-0/verify.js && jest -i --config ./module-0/jest.config.js",
    "test": "jest",
    "test-watch-all": "jest --watch",
    "test-module-1": "jest --watch -i --config ./module-1/jest.config.js",
    "test-module-2": "jest --watch -i --config ./module-2/jest.config.js",
    "test-module-3": "jest --watch -i --config ./module-3/jest.config.js",
    "test-module-4": "jest --watch -i --config ./module-4/jest.config.js",
    "build-module-4": "webpack --config ./module-4/webpack.config.js",
    "serve-module-4": "webpack-dev-server --config ./module-4/webpack.config.js",
    "test-module-5ab": "jest --watch -i --config ./module-5ab/jest.config.js",
    "build-module-5ab": "webpack --config ./module-5ab/webpack.config.js",
    "serve-module-5ab": "concurrently --names \"WEB,API\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run serve-module-5ab-web\" \"npm run serve-module-5ab-api\"",
    "serve-module-5ab-web": "webpack-dev-server --config ./module-5ab/webpack.config.js",
    "serve-module-5ab-api": "nodemon --exec babel-node --presets=env -- ./module-5ab/api/server.js",
    "test-module-5c": "jest --watch -i --config ./module-5c/jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pepopowitz/tdd-workshop.git"
  },
  "keywords": [
    "TDD",
    "JavaScript",
    "workshop",
    "Jest"
  ],
  "author": "Steven J Hicks",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pepopowitz/tdd-workshop/issues"
  },
  "homepage": "https://github.com/pepopowitz/tdd-workshop#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^2.0.2",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "html-looks-like": "^1.0.3",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.4.4",
    "axios": "^0.18.0",
    "body-parser": "^1.19.0",
    "express": "^4.16.4",
    "form-serialize": "^0.7.2",
    "lodash": "^4.17.11"
  }
}
