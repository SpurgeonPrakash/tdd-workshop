{
  "name": "tdd-workshop",
  "version": "1.0.0",
  "description": "Materials for the workshop \"Building Quality JavaScript With Test-Driven Development\", by Steven J Hicks.",
  "private": true,
  "scripts": {
    "verify": "node ./module-0/verify.js",
    "test": "jest",
    "test-watch-all": "jest --watch",
    "test-module-1": "jest --watch -i --config ./module-1/jest.config.js",
    "test-module-2": "jest --watch -i --config ./module-2/jest.config.js",
    "test-module-3": "jest --watch -i --config ./module-3/jest.config.js",
    "test-module-4": "jest --watch -i --config ./module-4/jest.config.js",
    "build-module-4": "webpack --config ./module-4/webpack.config.js",
    "serve-module-4": "webpack-dev-server --config ./module-4/webpack.config.js",
    "test-module-5a": "jest --watch -i --config ./module-5a/jest.config.js",
    "build-module-5a": "webpack --config ./module-5a/webpack.config.js",
    "serve-module-5a": "concurrently --names \"WEB,API\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run serve-module-5a-web\" \"npm run serve-module-5a-api\"",
    "serve-module-5a-web": "webpack-dev-server --config ./module-5a/webpack.config.js",
    "serve-module-5a-api": "nodemon --exec babel-node --presets=env -- ./module-5a/api/server.js",
    "test-module-5b": "jest --watch -i --config ./module-5b/jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pepopowitz/tdd-workshop.git"
  },
  "keywords": [
    "TDD",
    "JavaScript",
    "workshop",
    "Jest"
  ],
  "author": "Steven J Hicks",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pepopowitz/tdd-workshop/issues"
  },
  "homepage": "https://github.com/pepopowitz/tdd-workshop#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "html-looks-like": "^1.0.3",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "nodemon": "^1.17.4",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "form-serialize": "^0.7.2",
    "lodash": "^4.17.10"
  }
}
